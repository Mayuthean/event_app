<h1><%=@event.title%></h1>
<%=form_for([@event, @registration]) do |f|%>
  <%=f.label :Name%>
  <%=f.text_field :name %><br><br>
  <%=f.label :Gender%>
  <%=f.text_field :gender%><br><br>
  <%=f.label :Phone%>
  <%=f.text_field :phone%><br><br>
  <%=f.label :Email%>
  <%=f.email_field :email%><br><br>
  <%=f.submit%>
<%end%>

